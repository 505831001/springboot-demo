<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.erp.stock.mapper.StkIoBillMapper">
    <select id="selectByStockIoType" parameterType="java.lang.String" resultType="org.jeecg.erp.stock.entity.StkIoBill">
		SELECT *
		  FROM stk_io_bill
		 WHERE stock_io_type = #{stockIoType}
	</select>

</mapper>